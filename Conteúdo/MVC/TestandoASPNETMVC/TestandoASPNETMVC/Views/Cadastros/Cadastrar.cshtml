@using TestandoASPNETMVC.Models

@model TestandoASPNETMVC.Cliente

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@using (Html.BeginForm("Cadastrar", "Cadastros", FormMethod.Post))
{
    @Html.DisplayNameFor(c => c.Nome) <br />
    @Html.TextBoxFor(c => c.Nome) <br />
    @Html.DisplayNameFor(e => e.Email) <br />
    @Html.TextBoxFor(e => e.Email) <br />

    <input type = "submit" value = "Cadastrar"/>
}